                            android:layout_marginEnd="@dimen/_7pxh"
                            android:layout_marginBottom="@dimen/_7pxv"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_twitter"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_twitter" />
                    </FrameLayout>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
